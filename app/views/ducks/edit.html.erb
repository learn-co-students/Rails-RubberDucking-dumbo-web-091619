<h1>Edit Duck Form</h1>
<%= form_for @duck do |f| %>
<%= f.label :name %>
<%= f.text_field :name, :required => 'required' %> <br>

<%= f.label :description %>
<%= f.text_field :description, :required => 'required' %> <br>

<%= f.label :student_id %>
<%= f.number_field :student_id %>

<%= f.submit %>
<% end %>